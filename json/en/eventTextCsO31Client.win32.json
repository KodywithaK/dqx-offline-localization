  "9042": {
    "<voice 00770_00009042><start_lip_sync n00129 _normal m00001>Mm-hmm. Gizark.\nYou've done me a great favor.<stop_lip_animation n00129 CLOSE_MOUTH>": ""
  },
  "9043": {
