  },
  "1333": {
    "<voice 04579_00001333><start_lip_sync m50008 _normal m00001>King Grossner: captured<heart><stop_lip_animation m50008 CLOSE_MOUTH>": ""
  },
  "1334": {
    "<voice 04739_00001334><start_lip_sync m50008 _normal m00001>Oh, Maille.<stop_lip_animation m50008 CLOSE_MOUTH>": ""
  },
  "1335": {
